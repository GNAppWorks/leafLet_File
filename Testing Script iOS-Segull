
var target = UIATarget.localTarget();
// speed is in meters/sec  
 var points = [  
                 {location:{latitude:38.345224,longitude:-75.605764}},  
                 ];

 for (var i = 0; i < points.length; i++)  
 {  
      target.setLocationWithOptions(points[i].location);  
      target.delay(10.0);  
 }


